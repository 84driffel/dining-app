<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.dininghallapplication.ui.home.HomeFragment"
        android:label="Burge"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.dininghallapplication.ui.dashboard.DashboardFragment"
        android:label="Catlett"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.dininghallapplication.ui.notifications.NotificationsFragment"
        android:label="Hillcrest"
        tools:layout="@layout/fragment_notifications" />

    <fragment
        android:id="@+id/navigation_custom"
        android:name="com.example.dininghallapplication.ui.custom.CustomFragment"
        android:label="Custom"
        tools:layout="@xml/root_preferences" />
</navigation>